version: '3'

services:
  transmission:
    image: linuxserver/transmission
    container_name: transmission

    environment:
      - PUID=1000
      - PGID=1000
      - TZ=America/Argentina/Buenos_Aires
    volumes:
      - /home/walli/.docker_config/trasmission/config:/config
      - /home/walli/Descargas:/downloads
    ports:
      - "9001:9091"
      
    restart: unless-stopped


version: "3.8"
services:
  raspap:
    container_name: raspap
    image: ghcr.io/raspap/raspap-docker:latest

    privileged: true
    network_mode: host
    cgroup: host # uncomment when using an ARM device 
    cap_add:
      - SYS_ADMIN
    volumes:
      - /home/walli/.docker_config/raspap/cgroup:/sys/fs/cgroup:rw
    restart: unless-stopped


version: '3.7'

services:
  photoprism:
    image: photoprism/photoprism:latest
    container_name: photoprism
    restart: unless-stopped
    ports:
      - "9011:2342"
    volumes:
      - /home/walli/Imagenes:/photoprism/storage
    environment:
      - PHOTOPRISM_ADMIN_PASSWORD=diron7547


version: '3'

services:
  navidrome:
    image: deluan/navidrome:latest
    container_name: navidrome
    restart: unless-stopped
    ports:
      - "9006:4533"
    volumes:
      - /home/walli//Musica:/music
      - /home/walli/.docker_config/navidrome:/config
    environment:
      - TZ=America/Argentina/Buenos_Aires
    network_mode: bridge


version: "3"
services:
  metube:
    image: ghcr.io/alexta69/metube
    container_name: metube
    restart: unless-stopped
    ports:
      - "9010:8081"
    volumes:
      - /home/walli/Descargas:/downloads


version: '3'

services:
  jellyfin:
    image: jellyfin/jellyfin:latest
    container_name: jellyfin
    restart: unless-stopped
    ports:
      - "9003:8096"

    volumes:
      - /home/walli/.docker_config/jellyfin/:/config
      - /home/walli/:/media
    environment:
      - TZ= America/Argentina/Buenos_Aires
    network_mode: bridge


version: '3'

services:
  homarr:
    container_name: homarr
    image: ghcr.io/ajnart/homarr:latest
    restart: unless-stopped
    volumes:
      - /home/walli/.docker_config/docker.sock:/var/run/docker.sock # Optional, only if you want docker integration
      - /home/walli/.docker_config/homarr/configs:/app/data/configs
      - /home/walli/.docker_config/homarr/icons:/app/public/icons
      - /home/walli/.docker_config/homarr/data:/data
    ports:
      - '9004:7575'


version: "3"

services:
  filebrowser:
    image: hurlenko/filebrowser
    container_name: filebrowser
    user: "${UID}:${GID}"
    ports:
      - 9002:8080
    volumes:
      - /home/walli/:/data
      - /home/walli/.docker_config/filebrowser/:/config
    environment:
      - FB_BASEURL=/filebrowser
    restart: always


version: '3.3'

services:
  deemix:
      image: finniedj/deemix
      container_name: deemix
      volumes:
          - /home/walli//Musica:/downloads
          - /home/walli/.docker_config/deemix/:/config
      environment:
          - PUID=1000
          - PGID=1000
          - UMASK_SET=022
      ports:
          - 9005:6595


